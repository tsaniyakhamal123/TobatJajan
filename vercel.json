{
  "version": 2,
  "builds": [
    {
      "src": "frontend/tobatjajan-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "cd frontend/tobatjajan-frontend && npm install && npm run build",
        "distDir": "frontend/tobatjajan-frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/tobatjajan-frontend/dist/index.html"
    }
  ]
}
